    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "train_landslide",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/landslide_train.py",
            "console": "integratedTerminal",
            "args": [
                "--do_finetune",
            ]
        }
    ]
}